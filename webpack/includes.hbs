<style>html { font-family: sans-serif; } .wf-loading { font-family: sans-serif !important; }</style>
{{#each htmlWebpackPlugin.files.css}}<link rel="stylesheet" type="text/css" href="{{this}}"></link>{{/each}}
{{#each htmlWebpackPlugin.files.js}}<script src="{{this}}" async="async"></script>{{/each}}
<script type="text/javascript" data-no-instant>
  function fire() {
    document.dispatchEvent(new CustomEvent('a.load'));
  }
  if (document.readyState === 'complete') {
    fire();
  } else {
    window.addEventListener('load', fire);
  }
</script>
