{
  "name": "@alexwilson/cms",
  "version": "1.0.0",
  "description": "Netlify-CMS distribution",
  "main": "index.js",
  "scripts": {
    "build": "parcel build ./client/index.html --public-url=/cms --out-dir=./client-dist --detailed-report --no-source-maps --log-level=4",
    "predeploy": "npm run build",
    "deploy": "wrangler publish",
    "start": "parcel ./client/index.html --out-dir=./client-dist --detailed-report"
  },
  "author": "Alex Wilson <alex@alexwilson.tech>",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@cloudflare/wrangler": "^1.15.0",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "netlify-cms": "^2.10.96",
    "netlify-cms-app": "^2.14.26",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "uuid": "^8.3.2"
  }
}
