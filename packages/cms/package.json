{
  "name": "@alexwilson/cms",
  "version": "1.0.0",
  "description": "Netlify-CMS distribution",
  "main": "index.js",
  "scripts": {
    "build": "parcel build ./client/index.html --public-url=https://static.alexwilson.tech/cms/ --detailed-report --no-source-maps --log-level=4",
    "deploy:client": "aws s3 sync --acl=public-read --delete ./dist/ s3://alex-static-assets/cms/",
    "deploy:worker": "wrangler publish",
    "deploy": "npm-run-all build deploy:*",
    "test": "npm run build",
    "start": "parcel ./client/index.html --log-level=4"
  },
  "author": "Alex Wilson <alex@alexwilson.tech>",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@cloudflare/wrangler": "^1.15.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "raw-loader": "^4.0.2",
    "webpack": "^5.30.0"
  },
  "dependencies": {
    "netlify-cms": "^2.10.111",
    "netlify-cms-app": "^2.14.40",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2"
  }
}
